<odoo>
    <data>
        <record id="view_cafe_taxline_form" model="ir.ui.view">
            <field name="name">cafe.taxline.form</field>
            <field name="model">cafe.taxline</field>
            <field name="arch" type="xml">
                <form string="Taxline Form">
                <sheet>
                    <group>
                        <field name="tax_id"/>
                        <field name="base_price"/>
                        <field name="percentage"/>
                        <field name="tax_amount"/>
                    </group>
                </sheet>
                </form>
            </field>
        </record>

        <record id="view_cafe_taxline_tree" model="ir.ui.view">
            <field name="name">cafe.taxline.tree</field>
            <field name="model">cafe.taxline</field>
            <field name="arch" type="xml">
                <tree string="Taxline List">
                        <field name="tax_id"/>
                        <field name="base_price"/>
                        <field name="percentage"/>
                        <field name="tax_amount"/>
                </tree>
            </field>
        </record>
    </data>

</odoo>