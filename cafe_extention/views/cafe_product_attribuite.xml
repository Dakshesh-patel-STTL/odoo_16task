<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_cafe_product_attribuite_tree" model="ir.ui.view">
        <field name="name">cafe.product.attribuite.tree</field>
        <field name="model">cafe.product.attribuite</field>
        <field name="arch" type="xml">
            <tree string="Product Attributes">
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="view_cafe_product_attribuite_form" model="ir.ui.view">
        <field name="name">cafe.product.attribuite.form</field>
        <field name="model">cafe.product.attribuite</field>
        <field name="arch" type="xml">
            <form string="Product Attribute Form">
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>


</odoo>