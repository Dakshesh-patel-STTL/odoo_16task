<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_cafe_product_attribuites_tree" model="ir.ui.view">
        <field name="name">cafe.product.attribuites.tree</field>
        <field name="model">cafe.product.attribuites</field>
        <field name="arch" type="xml">
            <tree string="Product Attributes and Values">
                <field name="attribuite"/>
                <field name="attribuite_values" widget="many2many_tags"/>
            </tree>
        </field>
    </record>
    
    <record id="view_cafe_product_attribuites_form" model="ir.ui.view">
        <field name="name">cafe.product.attribuites.form</field>
        <field name="model">cafe.product.attribuites</field>
        <field name="arch" type="xml">
            <form string="Product Attributes and Values Form">
                <sheet>
                    <group>
                        <field name="attribuite"/>
                        <field name="attribuite_values"/>
                        <field name="product_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    

</odoo>
